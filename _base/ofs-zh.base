views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("content/ofs")
        - '!file.name.contains("_index")'
        - file.name.contains("zh-CN")
    order:
      - file.name
      - file.folder
      - file.tags
    columnSize:
      file.name: 337
      file.folder: 228
